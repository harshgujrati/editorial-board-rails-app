  <header class="header-area">
  <!-- site-navbar start -->
  <div class="navbar-area">
    <div class="container">
      <nav class="site-navbar">
        <!-- site logo -->
        <a href="#home" class="site-logo"><%= image_tag("Logo.png", height: 50, width:50) %></a>

        <!-- site menu/nav -->
        <ul>
