<%= render partial: '/layouts/header2' %>
<h1 class="member-head align-center u-margin-bottom-small u-margin-top-medium">Our Team</h1>
<h1 class="member-head align-center u-margin-bottom-big">Final Year Members</h1>
<div class="member">
  <div class="box">
    <%= form_for(@final_year_members, multipart: true) do |member| %>
      <div class="card u-margin-bottom-medium">
        <div class="imgBx">
           <%= member.label :avatar %>
           <%= member.file_field :avatar %>
        </div>
        <div class="details">
            <h2><%= member.name %><br><span>Final Year</span></h2>
        </div>
      </div>
    <% end %>
       
  </div>
</div>

