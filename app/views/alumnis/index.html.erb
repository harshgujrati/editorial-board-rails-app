<p id="notice"><%= notice %></p>

<h1>Alumnis</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Profession</th>
      <th>Description</th>
      <th>Facebook url</th>
      <th>Linkedin url</th>
      <th>Twitter url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alumnis.each do |alumni| %>
      <tr>
        <td><%= alumni.name %></td>
        <td><%= alumni.profession %></td>
        <td><%= alumni.description %></td>
        <td><%= alumni.facebook_url %></td>
        <td><%= alumni.linkedin_url %></td>
        <td><%= alumni.twitter_url %></td>
        <td><%= link_to 'Show', alumni %></td>
        <td><%= link_to 'Edit', edit_alumni_path(alumni) %></td>
        <td><%= link_to 'Destroy', alumni, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alumni', new_alumni_path %>
